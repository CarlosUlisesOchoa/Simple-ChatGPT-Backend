{
  "name": "openai-node-server",
  "version": "1.1.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "echo Error: you should specify the environment, e.g. \"npm run start:dev\" or \"npm run start:prod\" && exit 1",
    "start:dev": "npm run dev",
    "start:prod": "npm run prod",
    "server-dev": "nodemon --exec \"node --env-file .env\" ./dist/index.js",
    "server-prod": "node --env-file .env ./dist/index.js",
    "dev": "cross-env NODE_ENV=development npm run dev-cmd",
    "dev-cmd": "npm run build && concurrently \"npm run tsc-watch\" \"npm run server-dev\"",
    "prod": "cross-env NODE_ENV=production npm run prod-cmd",
    "prod-cmd": "npm run build && npm run server-prod",
    "tsc-watch": "tsc --watch",
    "build": "tsc",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "Carlos Ulises Ochoa",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.30",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "concurrently": "^8.2.2",
    "cross-env": "^10.1.0",
    "eslint": "^8.57.0",
    "nodemon": "^3.1.11",
    "prettier": "^3.2.5",
    "standard": "^17.1.0",
    "typescript": "^5.4.3"
  },
  "dependencies": {
    "express": "5.2.1",
    "openai": "^6.9.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  }
}
